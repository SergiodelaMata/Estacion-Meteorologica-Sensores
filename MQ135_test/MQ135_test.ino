void setup() {
  Serial.begin(9600);
}

void loop() {
  
  int adc_MQ = analogRead(32); //Lemos la salida anal√≥gica del MQ
  float voltaje = adc_MQ * (5.0 / 1023.0); //Convertimos la lectura en un valor de voltaje

  Serial.print("adc:");
  Serial.print(adc_MQ);
  Serial.print("    voltaje:");
  Serial.println(voltaje);
  delay(3000);
}
